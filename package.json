{
  "name": "workflow_babel_n_webpack",
  "version": "1.0.0",
  "description": "Tutorial for Babel and Webpack with Ninja",
  "main": "index.js",
  "scripts": {
    "serve": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "babel": "babel src/index.js -w -o dist/assets/bundle.js",
    "babel2": "npx babel src/index.js -w -o dist/assets/bundle.js",
    "webpack3": "npx webpack --watch",
    "webpack2": "\"node_modules\\.bin\\webpack\" --watch",
    "babelTest": "node_modules/.bin/babel src/index.js -o dist/assets/bundle.js",
    "babelTest2": "\"node_modules\\.bin\\babel\" src/index.js -o dist/assets/bundle.js"
  },
  "author": "Luca Cezar",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "babel-loader": "^9.1.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "webpack-dev-server": "^5.0.3"
  }
}
